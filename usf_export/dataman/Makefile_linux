#
OBJ=	dataman.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	dataman
INCL=	Makefile_linux
SUBS = 
STATIC = 
SUBS = dataman_subs.o
INCL = Makefile_linux dataman.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

dataman.o:	dataman.fpp $(INCL)
	$(fc) $(opt) -c dataman.fpp

dataman_subs.o:	dataman_subs.fpp $(INCL)
			$(fc) $(opt) -c dataman_subs.fpp
