#
OBJ=	mappage.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	mappage
INCL=	Makefile_linux
SUBS = 
STATIC = 
##SUBS## SUBS = mappage_subs.o
##INCL## INCL = Makefile_linux mappage.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

mappage.o:	mappage.fpp $(INCL)
	$(fc) $(opt) -c mappage.fpp

##SUBS## mappage_subs.o:	mappage_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c mappage_subs.fpp
