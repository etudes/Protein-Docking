#
OBJ=	mapfix.o ../gklib/osx_kleylib  ../ccp4libs_latest_m32_osx/libccp4f.a  ../ccp4libs_latest_m32_osx/libccp4c.a
EXEC =	mapfix
INCL=	Makefile_osx
SUBS = 
##SUBS## SUBS = mapfix_subs.o
##INCL## INCL = Makefile_osx mapfix.incl
##STATIC## STATIC = -static

fc =	 gfortran -DOSX

opt= -O -u -ffixed-line-length-132 -m32 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) -m32 ; strip $(EXEC)

mapfix.o:	mapfix.fpp $(INCL)
	$(fc) $(opt) -c mapfix.fpp

##SUBS## mapfix_subs.o:	mapfix_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c mapfix_subs.fpp
