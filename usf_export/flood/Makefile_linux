OBJ=	flood.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	flood
INCL=	Makefile_linux
SUBS = 
STATIC = 
##SUBS## SUBS = flood_subs.o
##INCL## INCL = Makefile_linux flood.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

flood.o:	flood.fpp $(INCL)
	$(fc) $(opt) -c flood.fpp

##SUBS## flood_subs.o:	flood_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c flood_subs.fpp
