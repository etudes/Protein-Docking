#
OBJ=	lib.o ../gklib/osx_kleylib  ../ccp4libs_latest_m32_osx/libccp4f.a  ../ccp4libs_latest_m32_osx/libccp4c.a
EXEC =	lib
INCL=	Makefile_osx
SUBS = 
##SUBS## SUBS = lib_subs.o
##INCL## INCL = Makefile_osx lib.incl
##STATIC## STATIC = -static

fc =	 gfortran -DOSX

opt= -O -u -ffixed-line-length-132 -m32 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) -m32 ; strip $(EXEC)

lib.o:	lib.fpp $(INCL)
	$(fc) $(opt) -c lib.fpp

##SUBS## lib_subs.o:	lib_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c lib_subs.fpp
