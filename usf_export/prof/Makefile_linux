#
OBJ=	prof.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	prof
INCL=	Makefile_linux
SUBS = 
STATIC = 
##SUBS## SUBS = prof_subs.o
##INCL## INCL = Makefile_linux prof.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

prof.o:	prof.fpp $(INCL)
	$(fc) $(opt) -c prof.fpp

##SUBS## prof_subs.o:	prof_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c prof_subs.fpp
