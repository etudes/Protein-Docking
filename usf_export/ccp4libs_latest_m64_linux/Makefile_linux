OBJ=	ccp4libs_latest_m64_linux.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	ccp4libs_latest_m64_linux
INCL=	Makefile_linux
SUBS = 
STATIC = 
##SUBS## SUBS = ccp4libs_latest_m64_linux_subs.o
##INCL## INCL = Makefile_linux ccp4libs_latest_m64_linux.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -check_bounds -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

ccp4libs_latest_m64_linux.o:	ccp4libs_latest_m64_linux.fpp $(INCL)
	$(fc) $(opt) -c ccp4libs_latest_m64_linux.fpp

##SUBS## ccp4libs_latest_m64_linux_subs.o:	ccp4libs_latest_m64_linux_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c ccp4libs_latest_m64_linux_subs.fpp
