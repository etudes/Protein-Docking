#
OBJ=	odbman.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	odbman
INCL=	Makefile_linux
SUBS = 
STATIC = 
##SUBS## SUBS = odbman_subs.o
INCL = Makefile_linux odbman.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

odbman.o:	odbman.fpp $(INCL)
	$(fc) $(opt) -c odbman.fpp

##SUBS## odbman_subs.o:	odbman_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c odbman_subs.fpp
