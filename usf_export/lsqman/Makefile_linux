#
OBJ=	lsqman.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	lsqman
INCL=	Makefile_linux
SUBS = 
STATIC = 
SUBS = lsqman_subs.o
INCL = Makefile_linux lsqman.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

lsqman.o:	lsqman.fpp $(INCL)
	$(fc) $(opt) -c lsqman.fpp

lsqman_subs.o:	lsqman_subs.fpp $(INCL)
			$(fc) $(opt) -c lsqman_subs.fpp
