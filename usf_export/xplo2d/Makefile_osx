#
OBJ=	xplo2d.o ../gklib/osx_kleylib  ../ccp4libs_latest_m32_osx/libccp4f.a  ../ccp4libs_latest_m32_osx/libccp4c.a
EXEC =	xplo2d
INCL=	Makefile_osx
SUBS = 
##SUBS## SUBS = xplo2d_subs.o
##INCL## INCL = Makefile_osx xplo2d.incl
##STATIC## STATIC = -static

fc =	 gfortran -DOSX

opt= -O -u -ffixed-line-length-132 -m32 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) -m32 ; strip $(EXEC)

xplo2d.o:	xplo2d.fpp $(INCL)
	$(fc) $(opt) -c xplo2d.fpp

##SUBS## xplo2d_subs.o:	xplo2d_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c xplo2d_subs.fpp
