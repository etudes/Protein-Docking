#
OBJ=	aconio.o ../gklib/linux_kleylib  ../ccp4libs_latest_m64_linux/libccp4f.a  ../ccp4libs_latest_m64_linux/libccp4c.a
EXEC =	aconio
INCL=	Makefile_linux
SUBS = 
STATIC = 
##SUBS## SUBS = aconio_subs.o
##INCL## INCL = Makefile_linux aconio.incl
##STATIC## STATIC = -static

fc =	 gfortran -DLINUX -m64

opt= -O -u -ffixed-line-length-132 -I..
link= -O

$(EXEC):	$(OBJ) $(SUBS)
	$(fc) $(link) -o $(EXEC) $(SUBS) $(OBJ) $(STATIC) ; strip $(EXEC)

aconio.o:	aconio.fpp $(INCL)
	$(fc) $(opt) -c aconio.fpp

##SUBS## aconio_subs.o:	aconio_subs.fpp $(INCL)
##SUBS## 			$(fc) $(opt) -c aconio_subs.fpp
