mkdir /home/vikram/Protein-Docking/1ibrresults
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.PsuedoProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1IBR A B
mv /home/vikram/Protein-Docking/1ibrnative.txt /home/vikram/Protein-Docking/1ibrresults/1ibrnative.txt
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1IBR AB 1QC4 A 1F59 A 4
cd /home/vikram/Protein-Docking/usf_export/lsqman
for i in $(seq 1000)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/ib/pdb1ibr.ent \n re m2 /home/vikram/Protein-Docking/result1ibrmodel$i.pdb \n ex m1 \"a1-169 b1-458\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp1ibr.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp1ibr.txt /home/vikram/Protein-Docking/result1ibr /home/vikram/Protein-Docking/scorerms1ibr.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp1ibr.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 1000) 
do
	mv /home/vikram/Protein-Docking/result1ibrmodel$i.pdb /home/vikram/Protein-Docking/1ibrresults/result1ibrmodel$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp1ibr.txt /home/vikram/Protein-Docking/1ibrresults/scorermstmp1ibr.txt
mv /home/vikram/Protein-Docking/scorerms1ibr.txt /home/vikram/Protein-Docking/1ibrresults/scorerms1ibr.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/1ibrresults/result1ibrmodel$i.pdb /home/vikram/Protein-Docking/tmp/1/result1ibrmodel$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result1ibrmodel${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result1ibrmodel${i}scwrl.pdb > /home/vikram/Protein-Docking/tmp/$j/result1ibrmodel${i}scwrl.txt
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/result1ibrmodel${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/result1ibrmodel$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/11/result1ibrmodel$i.pdb /home/vikram/Protein-Docking/result1ibrmodel${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/1ibrresults
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/ib/pdb1ibr.ent \n re m2 /home/vikram/Protein-Docking/result1ibrmodel${i}iter.pdb \n ex m1 \"a1-169 b1-458\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp1ibr.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
java RMSEScoreGen /home/vikram/Protein-Docking/scorermsitertmp1ibr.txt /home/vikram/Protein-Docking/result1ibr /home/vikram/Protein-Docking/scorermsiter1ibr.txt
java MinRMS /home/vikram/Protein-Docking/scorermsitertmp1ibr.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result1ibrmodel${i}iter.pdb /home/vikram/Protein-Docking/1ibrresults/result1ibrmodel${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp1ibr.txt /home/vikram/Protein-Docking/1ibrresults/scorermsitertmp1ibr.txt
mv /home/vikram/Protein-Docking/scorermsiter1ibr.txt /home/vikram/Protein-Docking/1ibrresults/scorermsiter1ibr.txt
