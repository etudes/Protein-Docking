mkdir /home/vikram/Protein-Docking/1jtgresults
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.PsuedoProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1JTG B A
mv /home/vikram/Protein-Docking/1jtgnative.txt /home/vikram/Protein-Docking/1jtgresults/1jtgnative.txt
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1JT 3GMU B 1ZG4 A 4
cd /home/vikram/Protein-Docking/usf_export/lsqman
for i in $(seq 1000)
do
	echo -e "re m1 ../../pdb.unzipped/wwpdb/pdb/jt/pdb1jtg.ent \n re m2 ../../result1jtgmodel$i.pdb \n ex m1 \"b1-165 a1-263\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp1jtg.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp1jtg.txt /home/vikram/Protein-Docking/result1jtg /home/vikram/Protein-Docking/scorerms1jtg.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp1jtg.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 1000) 
do
	mv /home/vikram/Protein-Docking/result1jtgmodel$i.pdb /home/vikram/Protein-Docking/1jtgresults/result1jtgmodel$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp1jtg.txt /home/vikram/Protein-Docking/1jtgresults/scorermstmp1jtg.txt
mv /home/vikram/Protein-Docking/scorerms1jtg.txt /home/vikram/Protein-Docking/1jtgresults/scorerms1jtg.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/1jtgresults/result1jtgmodel$i.pdb /home/vikram/Protein-Docking/tmp/1/result1jtgmodel$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result1jtgmodel${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result1jtgmodel${i}scwrl.pdb > /home/vikram/Protein-Docking/tmp/$j/result1jtgmodel${i}scwrl.txt
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/result1jtgmodel${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/result1jtgmodel$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/11/result1jtgmodel$i.pdb /home/vikram/Protein-Docking/result1jtgmodel${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/1jtgresults
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/jt/pdb1jtg.ent \n re m2 /home/vikram/Protein-Docking/result1jtgmodel${i}iter.pdb \n ex m1 \"a1-159 b1-214 c1-220\" m2 \"a1-214 b1-220 c1-159\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp1jtg.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
java RMSEScoreGen /home/vikram/Protein-Docking/scorermsitertmp1jtg.txt /home/vikram/Protein-Docking/result1jtg /home/vikram/Protein-Docking/scorermsiter1jtg.txt
java MinRMS /home/vikram/Protein-Docking/scorermsitertmp1jtg.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result1jtgmodel${i}iter.pdb /home/vikram/Protein-Docking/1jtgresults/result1jtgmodel${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp1jtg.txt /home/vikram/Protein-Docking/1jtgresults/scorermsitertmp1jtg.txt
mv /home/vikram/Protein-Docking/scorermsiter1jtg.txt /home/vikram/Protein-Docking/1jtgresults/scorermsiter1jtg.txt
