mkdir /home/vikram/Protein-Docking/1fqjresults
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.PsuedoProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1FQJ A B
mv /home/vikram/Protein-Docking/1fqjnative.txt /home/vikram/Protein-Docking/1fqjresults/1fqjnative.txt
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1FQJ AB 1TND C 1FQI A 4
cd /home/vikram/Protein-Docking/usf_export/lsqman
for i in $(seq 1000)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/av/pdb1fqj.ent \n re m2 /home/vikram/Protein-Docking/result1fqjmodel$i.pdb \n ex m1 \"a1-325 b1-147\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp1fqj.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp1fqj.txt /home/vikram/Protein-Docking/result1fqj /home/vikram/Protein-Docking/scorerms1fqj.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp1fqj.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 1000) 
do
	mv /home/vikram/Protein-Docking/result1fqjmodel$i.pdb /home/vikram/Protein-Docking/1fqjresults/result1fqjmodel$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp1fqj.txt /home/vikram/Protein-Docking/1fqjresults/scorermstmp1fqj.txt
mv /home/vikram/Protein-Docking/scorerms1fqj.txt /home/vikram/Protein-Docking/1fqjresults/scorerms1fqj.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/1fqjresults/result1fqjmodel$i.pdb /home/vikram/Protein-Docking/tmp/1/result1fqjmodel$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result1fqjmodel${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result1fqjmodel${i}scwrl.pdb > /home/vikram/Protein-Docking/tmp/$j/result1fqjmodel${i}scwrl.txt
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/result1fqjmodel${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/result1fqjmodel$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/11/result1fqjmodel$i.pdb /home/vikram/Protein-Docking/result1fqjmodel${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/1fqjresults
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/av/pdb1fqj.ent \n re m2 /home/vikram/Protein-Docking/result1fqjmodel${i}iter.pdb \n ex m1 \"a1-223 b1-181\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp1fqj.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
java RMSEScoreGen /home/vikram/Protein-Docking/scorermsitertmp1fqj.txt /home/vikram/Protein-Docking/result1fqj /home/vikram/Protein-Docking/scorermsiter1fqj.txt
java MinRMS /home/vikram/Protein-Docking/scorermsitertmp1fqj.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result1fqjmodel${i}iter.pdb /home/vikram/Protein-Docking/1fqjresults/result1fqjmodel${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp1fqj.txt /home/vikram/Protein-Docking/1fqjresults/scorermsitertmp1fqj.txt
mv /home/vikram/Protein-Docking/scorermsiter1fqj.txt /home/vikram/Protein-Docking/1fqjresults/scorermsiter1fqj.txt
