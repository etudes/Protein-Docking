mkdir /home/vikram/Protein-Docking/1f51results
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.PsuedoProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1F51 AB E
mv /home/vikram/Protein-Docking/1f51native.txt /home/vikram/Protein-Docking/1f51results/1f51native.txt
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1F51 ABE 1IXM AB 1SRR C 4
cd /home/vikram/Protein-Docking/usf_export/lsqman
for i in $(seq 1000)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/f5/pdb1f51.ent \n re m2 /home/vikram/Protein-Docking/result1f51model$i.pdb \n ex m1 \"a1-182 b1-182 c1-182\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp1f51.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp1f51.txt /home/vikram/Protein-Docking/result1f51 /home/vikram/Protein-Docking/scorerms1f51.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp1f51.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 1000) 
do
	mv /home/vikram/Protein-Docking/result1f51model$i.pdb /home/vikram/Protein-Docking/1f51results/result1f51model$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp1f51.txt /home/vikram/Protein-Docking/1f51results/scorermstmp1f51.txt
mv /home/vikram/Protein-Docking/scorerms1f51.txt /home/vikram/Protein-Docking/1f51results/scorerms1f51.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/1f51results/result1f51model$i.pdb /home/vikram/Protein-Docking/tmp/1/result1f51model$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result1f51model${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result1f51model${i}scwrl.pdb > /home/vikram/Protein-Docking/tmp/$j/result1f51model${i}scwrl.txt
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/result1f51model${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/result1f51model$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/11/result1f51model$i.pdb /home/vikram/Protein-Docking/result1f51model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/1f51results
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/f5/pdb1f51.ent \n re m2 /home/vikram/Protein-Docking/result1f51model${i}iter.pdb \n ex m1 \"a1-182 b1-182 c1-182\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp1f51.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
java RMSEScoreGen /home/vikram/Protein-Docking/scorermsitertmp1f51.txt /home/vikram/Protein-Docking/result1f51 /home/vikram/Protein-Docking/scorermsiter1f51.txt
java MinRMS /home/vikram/Protein-Docking/scorermsitertmp1f51.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result1f51model${i}iter.pdb /home/vikram/Protein-Docking/1f51results/result1f51model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp1f51.txt /home/vikram/Protein-Docking/1f51results/scorermsitertmp1f51.txt
mv /home/vikram/Protein-Docking/scorermsiter1f51.txt /home/vikram/Protein-Docking/1f51results/scorermsiter1f51.txt
