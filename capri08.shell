for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result08model${i}iter.pdb /home/vikram/Protein-Docking/result08/result08model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp08.txt /home/vikram/Protein-Docking/result08/scorermsitertmp08.txt
mv /home/vikram/Protein-Docking/scorermsiter08.txt /home/vikram/Protein-Docking/result08/scorermsiter08.txt
