for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result35model${i}iter.pdb /home/vikram/Protein-Docking/result35/result35model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp35.txt /home/vikram/Protein-Docking/result35/scorermsitertmp35.txt
