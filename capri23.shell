java MinRMS /home/vikram/Protein-Docking/scorermsitertmp23.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result23model${i}iter.pdb /home/vikram/Protein-Docking/result23/result23model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp23.txt /home/vikram/Protein-Docking/result23/scorermsitertmp23.txt
mv /home/vikram/Protein-Docking/scorermsiter23.txt /home/vikram/Protein-Docking/result23/scorermsiter23.txt
