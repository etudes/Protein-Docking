mkdir /home/vikram/Protein-Docking/result02/
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/ 02 4
cd /home/vikram/Protein-Docking/usf_export/lsqman
for i in $(seq 10)
do
	echo -e "re m1 ../../pdb.unzipped/wwpdb/pdb/ke/pdb1ken.ent \n re m2 ../../result02model$i.pdb \n ex m1 \"a1-328 b1-175 c1-328 d1-175 e1-328 f1-175 g1-212 h1-221\" m2 \"a1 b1 c1 d1 e1 f1 g1 h1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp02.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp02.txt /home/vikram/Protein-Docking/result02 /home/vikram/Protein-Docking/scorerms02.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp02.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result02model$i.pdb /home/vikram/Protein-Docking/result02/result02model$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp02.txt /home/vikram/Protein-Docking/result02/scorermstmp02.txt
mv /home/vikram/Protein-Docking/scorerms02.txt /home/vikram/Protein-Docking/result02/scorerms02.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/result02/result02model$i.pdb /home/vikram/Protein-Docking/tmp/1/result02model$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result02model${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result02model${i}scwrl.pdb > /home/vikram/Protein-Docking/tmp/$j/result02model${i}scwrl.txt
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/result02model${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/result02model$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/11/result02model$i.pdb /home/vikram/Protein-Docking/result02model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/result02
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/ke/pdb1ken.ent \n re m2 /home/vikram/Protein-Docking/result02model${i}iter.pdb \n ex m1 \"a1-328 b1-175 c1-328 d1-175 e1-328 f1-175 g1-212 h1-221\" m2 \"a1 b1 c1 d1 e1 f1 g1 h1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp02.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
java RMSEScoreGen /home/vikram/Protein-Docking/scorermsitertmp02.txt /home/vikram/Protein-Docking/result02 /home/vikram/Protein-Docking/scorermsiter02.txt
java MinRMS /home/vikram/Protein-Docking/scorermsitertmp02.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result02model${i}iter.pdb /home/vikram/Protein-Docking/result02/result02model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp02.txt /home/vikram/Protein-Docking/result02/scorermsitertmp02.txt
mv /home/vikram/Protein-Docking/scorermsiter02.txt /home/vikram/Protein-Docking/result02/scorermsiter02.txt
