do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/result38/result38model$i.pdb /home/vikram/Protein-Docking/tmp/1/result38model$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$j/
		mv /home/vikram/Protein-Docking/tmp/$j/result38model${i}.kb /home/vikram/Protein-Docking/tmp/$j/result38model${i}kb.pdb
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result38model${i}kb.pdb -o /home/vikram/Protein-Docking/tmp/$k/result38model${i}.pdb > /home/vikram/Protein-Docking/tmp/$j/result38model${i}scwrl.txt
	done
	mv /home/vikram/Protein-Docking/tmp/11/result38model$i.pdb /home/vikram/Protein-Docking/result38model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/result38
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/fm/pdb3fm8.ent \n re m2 /home/vikram/Protein-Docking/result38model${i}iter.pdb \n ex m1 \"a1-386 b1-104\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
