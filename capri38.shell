for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result38model${i}iter.pdb /home/vikram/Protein-Docking/result38/result38model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp38.txt /home/vikram/Protein-Docking/result38/scorermsitertmp38.txt
