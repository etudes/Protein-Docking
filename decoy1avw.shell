PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 1000)
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	mv $(printf /disk2/bound_perturb/1AVW/aa1AVW.ppk_%04d.pdb $i) $(printf /home/vikram/Protein-Docking/tmp/1/aa1avw.ppk_%d.pdb $i)
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/aa1avw.ppk_$i.pdb -o /home/vikram/Protein-Docking/tmp/$k/aa1avw.ppk_${i}scwrl.pdb
		#rm /home/vikram/Protein-Docking/tmp/result1avwmodel$i.pdb
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/aa1avw.ppk_${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/aa1avw.ppk_$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/1/aa1avw.ppk_${i}.pdb $(printf /disk2/bound_perturb/1AVW/aa1AVW.ppk_%04d.pdb $i)
	mv /home/vikram/Protein-Docking/tmp/11/aa1avw.ppk_${i}.pdb $(printf /disk2/bound_perturb/1AVW/aa1AVW.ppk_{%04d}kb.pdb $i)
	rm -rf /home/vikram/Protein-Docking/tmp
done
java -cp org.vikramdock.ProteinDock.jar org.vikramdock.PsuedoProteinDockPredict /home/vikram/Protein-Docking/ /disk2/bound_perturb/ 1AVW A D
