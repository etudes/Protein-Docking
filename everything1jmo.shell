mkdir /home/vikram/Protein-Docking/1jmoresults
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.PsuedoProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1JMO A HL
mv /home/vikram/Protein-Docking/1jmonative.txt /home/vikram/Protein-Docking/1jmoresults/1jmonative.txt
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1JMO AHL 1JMJ A 2CN0 HL 4
cd /home/vikram/Protein-Docking/usf_export/lsqman
for i in $(seq 1000)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/jm/pdb1jmo.ent \n re m2 /home/vikram/Protein-Docking/result1jmomodel$i.pdb \n ex m1 \"a1-427 b1-233 c1-14\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp1jmo.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp1jmo.txt /home/vikram/Protein-Docking/result1jmo /home/vikram/Protein-Docking/scorerms1jmo.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp1jmo.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 1000) 
do
	mv /home/vikram/Protein-Docking/result1jmomodel$i.pdb /home/vikram/Protein-Docking/1jmoresults/result1jmomodel$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp1jmo.txt /home/vikram/Protein-Docking/1jmoresults/scorermstmp1jmo.txt
mv /home/vikram/Protein-Docking/scorerms1jmo.txt /home/vikram/Protein-Docking/1jmoresults/scorerms1jmo.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/1jmoresults/result1jmomodel$i.pdb /home/vikram/Protein-Docking/tmp/1/result1jmomodel$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result1jmomodel${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result1jmomodel${i}scwrl.pdb > /home/vikram/Protein-Docking/tmp/$j/result1jmomodel${i}scwrl.txt
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/result1jmomodel${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/result1jmomodel$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/11/result1jmomodel$i.pdb /home/vikram/Protein-Docking/result1jmomodel${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/1jmoresults
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/jm/pdb1jmo.ent \n re m2 /home/vikram/Protein-Docking/result1jmomodel${i}iter.pdb \n ex m1 \"a1-427 b1-233 c1-14\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp1jmo.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
java RMSEScoreGen /home/vikram/Protein-Docking/scorermsitertmp1jmo.txt /home/vikram/Protein-Docking/result1jmo /home/vikram/Protein-Docking/scorermsiter1jmo.txt
java MinRMS /home/vikram/Protein-Docking/scorermsitertmp1jmo.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result1jmomodel${i}iter.pdb /home/vikram/Protein-Docking/1jmoresults/result1jmomodel${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp1jmo.txt /home/vikram/Protein-Docking/1jmoresults/scorermsitertmp1jmo.txt
mv /home/vikram/Protein-Docking/scorermsiter1jmo.txt /home/vikram/Protein-Docking/1jmoresults/scorermsiter1jmo.txt
