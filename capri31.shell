for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result31model${i}iter.pdb /home/vikram/Protein-Docking/result31/result31model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp31.txt /home/vikram/Protein-Docking/result31/scorermsitertmp31.txt
mv /home/vikram/Protein-Docking/scorermsiter31.txt /home/vikram/Protein-Docking/result31/scorermsiter31.txt
