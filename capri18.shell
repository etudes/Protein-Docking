mkdir /home/vikram/Protein-Docking/result18/
java CapriCleaner 18
mv /home/vikram/Protein-Docking/capri_18.brkFIXED /home/vikram/Protein-Docking/capri_18.brk
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/ 18 4
cd /home/vikram/Protein-Docking/bin32/lsqman
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/t6/pdb1t6g.ent \n re m2 /home/vikram/Protein-Docking/result18model$i.pdb \n ex m1 \"a1-184 b1-362\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
/home/vikram/Protein-Docking/bin32/lsqman < /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp18.txt
rm /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp18.txt /home/vikram/Protein-Docking/result18 /home/vikram/Protein-Docking/scorerms18.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp18.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result18model$i.pdb /home/vikram/Protein-Docking/result18/result18model$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp18.txt /home/vikram/Protein-Docking/result18/scorermstmp18.txt
mv /home/vikram/Protein-Docking/scorerms18.txt /home/vikram/Protein-Docking/result18/scorerms18.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/result18/result18model$i.pdb /home/vikram/Protein-Docking/tmp/1/result18model$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$j/
		mv /home/vikram/Protein-Docking/tmp/$j/result18model${i}.kb /home/vikram/Protein-Docking/tmp/$j/result18model${i}kb.pdb
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result18model${i}kb.pdb -o /home/vikram/Protein-Docking/tmp/$k/result18model${i}.pdb > /home/vikram/Protein-Docking/tmp/$j/result18model${i}scwrl.txt
	done
	mv /home/vikram/Protein-Docking/tmp/11/result18model$i.pdb /home/vikram/Protein-Docking/result18model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/result18
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/t6/pdb1t6g.ent \n re m2 /home/vikram/Protein-Docking/result18model${i}iter.pdb \n ex m1 \"a1-184 b1-362\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
/home/vikram/Protein-Docking/bin32/lsqman < /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp18.txt
rm /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
java RMSEScoreGenIter /home/vikram/Protein-Docking/scorermsitertmp18.txt /home/vikram/Protein-Docking/result18 /home/vikram/Protein-Docking/scorermsiter18.txt
java MinRMS /home/vikram/Protein-Docking/scorermsitertmp18.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result18model${i}iter.pdb /home/vikram/Protein-Docking/result18/result18model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp18.txt /home/vikram/Protein-Docking/result18/scorermsitertmp18.txt
mv /home/vikram/Protein-Docking/scorermsiter18.txt /home/vikram/Protein-Docking/result18/scorermsiter18.txt
