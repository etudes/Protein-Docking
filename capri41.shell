for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result41model${i}iter.pdb /home/vikram/Protein-Docking/result41/result41model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp41.txt /home/vikram/Protein-Docking/result41/scorermsitertmp41.txt
mv /home/vikram/Protein-Docking/scorermsiter41.txt /home/vikram/Protein-Docking/result41/scorermsiter41.txt
