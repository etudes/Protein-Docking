time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/pdb/wwpdb/pdb/ 1AHW 1FGN LH 1TFH A 4
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 100) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	mv result1ahwmodel$i.pdb /home/vikram/Protein-Docking/tmp/1/result1ahwmodel$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result1ahwmodel${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result1ahwmodel${i}scwrl.pdb
		#rm /home/vikram/Protein-Docking/tmp/result1ahwmodel$i.pdb
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/result1ahwmodel${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/result1ahwmodel$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/11/result1ahwmodel$i.pdb /home/vikram/Protein-Docking/result1ahwmodel$i.pdb
	#rm -rf /home/vikram/Protein-Docking/tmp
done
for i in $(seq 100)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/ah/pdb1ahw.ent \n re m2 /home/vikram/Protein-Docking/result1ahwmodel$i.pdb \n ex m1 \"a1-214 b1-214 c1-211\" m2 \"a1-214 b1-214 c1-211\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp1ahw.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
java RMSEScoreGen /home/vikram/Protein-Docking/scorermsitertmp1ahw.txt /home/vikram/Protein-Docking/result1ahw /home/vikram/Protein-Docking/scorersimter1ahw.txt
