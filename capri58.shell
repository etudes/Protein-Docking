	done
	mv /home/vikram/Protein-Docking/tmp/11/result58model$i.pdb /home/vikram/Protein-Docking/result58model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/result58
