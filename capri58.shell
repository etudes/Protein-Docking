do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/g9/pdb4g9s.ent \n re m2 /home/vikram/Protein-Docking/result58model$i.pdb \n ex m1 \"a1-181 b1-111 c1-111\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
done
