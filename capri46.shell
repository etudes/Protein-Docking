java MinRMS /home/vikram/Protein-Docking/scorermsitertmp46.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result46model${i}iter.pdb /home/vikram/Protein-Docking/result46/result46model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp46.txt /home/vikram/Protein-Docking/result46/scorermsitertmp46.txt
mv /home/vikram/Protein-Docking/scorermsiter46.txt /home/vikram/Protein-Docking/result46/scorermsiter46.txt
