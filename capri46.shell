mkdir /home/vikram/Protein-Docking/result46/
java CapriCleaner 46
mv /home/vikram/Protein-Docking/capri_46.brkFIXED /home/vikram/Protein-Docking/capri_46.brk
time java -cp ProteinDock.jar -Xmx7000m org.vikramdock.ProteinDockPredict /home/vikram/Protein-Docking/ /home/vikram/Protein-Docking/ 46 4
cd /home/vikram/Protein-Docking/bin32/lsqman
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/q8/pdb3q87.ent \n re m2 /home/vikram/Protein-Docking/result46model$i.pdb \n ex m1 \"a1-164 b1-125\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
/home/vikram/Protein-Docking/bin32/lsqman < /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp46.txt
rm /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
cd /home/vikram/Protein-Docking
java RMSEScoreGen /home/vikram/Protein-Docking/scorermstmp46.txt /home/vikram/Protein-Docking/result46 /home/vikram/Protein-Docking/scorerms46.txt
java MinRMS /home/vikram/Protein-Docking/scorermstmp46.txt /home/vikram/Protein-Docking/minrms.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result46model$i.pdb /home/vikram/Protein-Docking/result46/result46model$i.pdb
done
mv /home/vikram/Protein-Docking/scorermstmp46.txt /home/vikram/Protein-Docking/result46/scorermstmp46.txt
mv /home/vikram/Protein-Docking/scorerms46.txt /home/vikram/Protein-Docking/result46/scorerms46.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/result46/result46model$i.pdb /home/vikram/Protein-Docking/tmp/1/result46model$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$j/
		mv /home/vikram/Protein-Docking/tmp/$j/result46model${i}.kb /home/vikram/Protein-Docking/tmp/$j/result46model${i}kb.pdb
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result46model${i}kb.pdb -o /home/vikram/Protein-Docking/tmp/$k/result46model${i}.pdb > /home/vikram/Protein-Docking/tmp/$j/result46model${i}scwrl.txt
	done
	mv /home/vikram/Protein-Docking/tmp/11/result46model$i.pdb /home/vikram/Protein-Docking/result46model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/result46
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/q8/pdb3q87.ent \n re m2 /home/vikram/Protein-Docking/result46model${i}iter.pdb \n ex m1 \"a1-164 b1-125\" m2 \"a1 b1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
/home/vikram/Protein-Docking/bin32/lsqman < /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermsitertmp46.txt
