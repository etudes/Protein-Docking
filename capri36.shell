	done
	mv /home/vikram/Protein-Docking/tmp/11/result36model$i.pdb /home/vikram/Protein-Docking/result36model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/result36
