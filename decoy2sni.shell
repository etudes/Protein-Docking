PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in 10 18 22 27 33 34 40 46 60 71 80 81 90 94 97 103 104 107 109 113 133 146 154 167 173 176 182 186 200 204 207 233 240 250 262 269 271 281 286 287 291 293 295 297 311 312 315 327 337 345 367 382 385 386 388 389 408 414 419 421 428 429 435 437 447 450 455 457 461 462 473 484 487 494 503 505 506 517 523 529 533 536 543 548 549 551 579 594 606 609 618 626 631 645 658 659 666 679 681 698 707 726 737 743 766 774 775 779 804 805 809 815 818 825 826 831 834 840 849 850 855 857 868 871 877 883 886 892 902 903 914 925 926 930 933 934 937 947 948 950 951 953 954 972 979 981 988 993
do
	let "i=${i}+1"
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	mv $(printf /disk2/bound_perturb/2SNI/aa2SNI.ppk_%04d.pdb $i) $(printf /home/vikram/Protein-Docking/tmp/1/aa2sni.ppk_%d.pdb $i)
	for j in $(seq 10)
	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/aa2sni.ppk_$i.pdb -o /home/vikram/Protein-Docking/tmp/$k/aa2sni.ppk_${i}scwrl.pdb
		#rm /home/vikram/Protein-Docking/tmp/result2snimodel$i.pdb
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$k/
		mv /home/vikram/Protein-Docking/tmp/$k/aa2sni.ppk_${i}scwrl.kb /home/vikram/Protein-Docking/tmp/$k/aa2sni.ppk_$i.pdb
	done
	mv /home/vikram/Protein-Docking/tmp/1/aa2sni.ppk_$i.pdb $(printf /disk2/bound_perturb/2SNI/aa2SNI.ppk_%04d.pdb $i)
	mv /home/vikram/Protein-Docking/tmp/11/aa2sni.ppk_$i.pdb $(printf /disk2/bound_perturb/2SNI/aa2SNI.ppk_{%04d}kb.pdb $i)
	rm -rf /home/vikram/Protein-Docking/tmp
done
java SequenceCreator 2SNI
java -cp ProteinDock.jar org.vikramdock.PsuedoProteinDockPredict /home/vikram/Protein-Docking/ /disk2/bound_perturb/ 2SNI E I
