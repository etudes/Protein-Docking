for i in 2i9b_ea 2ido_ab 2ot3_ba
do
	echo -e "\n Result for $i" >> /home/vikram/Protein-Docking/p7resultstmp.txt
	cd /home/vikram/Protein-Docking/
	java BenchParser ${i:0:4}
	cd /home/vikram/Protein-Docking/bin32
	for j in 2 3 4 5 6
	do
		echo -e "$ gunzip -c ../pdb/wwpdb/pdb/${i:1:2}/pdb${i:0:4}.ent.gz > ../pdb/wwpdb/pdb/${i:1:2}/pdb${i:0:4}.ent \n re m1 ../pdb/wwpdb/pdb/${i:1:2}/pdb${i:0:4}.ent \n re m2 ../result${i:0:4}model$j.txt \n fa m1 \"${i:5}\" m2 a 100 10 30 d 2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
	done
	echo -e "\n qu" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
    ./lsqman < /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp >> /home/vikram/Protein-Docking/p7resultstmp.txt
    cd /home/vikram/Protein-Docking
    java RMSECleaner 7
    rm /home/vikram/Protein-Docking/p7resultstmp.txt
    for j in 1 2 3 4 5 6
    do
    	rm /home/vikram/Protein-Docking/result${i:0:4}model$j.txt
	done
done
