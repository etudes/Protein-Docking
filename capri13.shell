mv /home/vikram/Protein-Docking/scorerms13.txt /home/vikram/Protein-Docking/result13/scorerms13.txt
PATH=$PATH:/home/vikram/Protein-Docking/scwrl4/
for i in $(seq 10) 
do
	mkdir /home/vikram/Protein-Docking/tmp
	mkdir /home/vikram/Protein-Docking/tmp/1
	cp /home/vikram/Protein-Docking/result13/result13model$i.pdb /home/vikram/Protein-Docking/tmp/1/result13model$i.pdb
	for j in $(seq 10)
	do
		let "k=${j}+1"
		/home/vikram/Protein-Docking/kobamin/kobamin.sh /home/vikram/Protein-Docking/tmp/$j/
		mv /home/vikram/Protein-Docking/tmp/$j/result13model${i}.kb /home/vikram/Protein-Docking/tmp/$j/result13model${i}kb.pdb
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result13model${i}kb.pdb -o /home/vikram/Protein-Docking/tmp/$k/result13model${i}.pdb > /home/vikram/Protein-Docking/tmp/$j/result13model${i}scwrl.txt
	done
	mv /home/vikram/Protein-Docking/tmp/11/result13model$i.pdb /home/vikram/Protein-Docking/result13model${i}iter.pdb
	mv home/vikram/Protein-Docking/tmp/ /home/vikram/Protein-Docking/result13
done
for i in $(seq 10)
do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/yn/pdb1ynt.ent \n re m2 /home/vikram/Protein-Docking/result13model${i}iter.pdb \n ex m1 \"a1-289 b1-214 c1-218\" m2 \"a1 b1 c1\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/bin32/lsqmanrun.tmp
