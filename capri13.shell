java MinRMS /home/vikram/Protein-Docking/scorermsitertmp13.txt /home/vikram/Protein-Docking/minrmsiter.txt
for i in $(seq 10) 
do
	mv /home/vikram/Protein-Docking/result13model${i}iter.pdb /home/vikram/Protein-Docking/result13/result13model${i}iter.pdb
done
mv /home/vikram/Protein-Docking/scorermsitertmp13.txt /home/vikram/Protein-Docking/result13/scorermsitertmp13.txt
