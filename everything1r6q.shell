cd /home/vikram/Protein-Docking/usf_export/lsqman
for i in $(seq 1000)
do
	echo -e "re m1 ../../pdb.unzipped/wwpdb/pdb/r6/pdb1r6q.ent \n re m2 ../../result1r6qmodel$i.pdb \n ex m1 \"a1-143 c1-106\" m2 \"a1 b12\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
done
echo -e "\n qu" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
/home/vikram/Protein-Docking/usf_export/lsqman/lsqman < /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp >> /home/vikram/Protein-Docking/scorermstmp1r6q.txt
rm /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
