	do
		let "k=${j}+1"
		mkdir /home/vikram/Protein-Docking/tmp/$k
		Scwrl4 -i /home/vikram/Protein-Docking/tmp/$j/result1r6qmodel${i}.pdb -o /home/vikram/Protein-Docking/tmp/$k/result1r6qmodel${i}scwrl.pdb > /home/vikram/Protein-Docking/tmp/$j/result1r6qmodel${i}scwrl.txt
