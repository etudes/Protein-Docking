do
	echo -e "re m1 /home/vikram/Protein-Docking/pdb.unzipped/wwpdb/pdb/r6/pdb1r6q.ent \n re m2 /home/vikram/Protein-Docking/result1r6qmodel${i}iter.pdb \n ex m1 \"a1-143 c1-106\" m2 \"a1 b12\" 100 10 30 d 2 \n show m1 m2 \n apply m1 m2 \n de m1 \n de m2 \n" >> /home/vikram/Protein-Docking/usf_export/lsqman/lsqmanrun.tmp
